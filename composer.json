{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "arielmejiadev/larapex-charts": "^2.1",
        "bavix/laravel-wallet": "^10.1",
        "consoletvs/charts": "^6.8",
        "google/recaptcha": "^1.3",
        "guzzlehttp/guzzle": "^7.9",
        "hibit-dev/geodetect": "^2.13",
        "illuminate/database": "^11.0",
        "laravel/fortify": "^1.25",
        "laravel/framework": "^11.0",
        "laravel/jetstream": "^5.0",
        "laravel/sanctum": "^4.0",
        "laravel/telescope": "^5.3",
        "laravel/tinker": "^2.8",
        "livewire/livewire": "^3.0",
        "paypal/rest-api-sdk-php": "^1.6",
        "qcod/laravel-settings": "^1.2",
        "reefki/laravel-device-detector": "^1.0",
        "spatie/laravel-honeypot": "^4.5",
        "spatie/laravel-permission": "^6.0",
        "spatie/laravel-sitemap": "^7.0",
        "spatie/laravel-webhook-client": "^3.3",
        "srmklive/paypal": "~3.0",
        "stancl/tenancy": "^3.8",
        "tuytoosh/active": "^1.8",
        "yajra/laravel-datatables": "11.0",
        "yajra/laravel-datatables-buttons": "^11.2",
        "yajra/laravel-datatables-oracle": "11.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.24.1",
        "laravel/pint": "^1.18",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "platform-check": false,
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
        
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
